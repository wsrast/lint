{
	"name": "@wsrast/lint",
	"version": "1.0.9",
	"author": "Wesley Rast",
	"description": "A boilerplate setup for linting and testing using Jest, prettier, Airbnb settings and Git hooks.",
	"repository": {
		"type": "git",
		"url": "https://github.com/wsrast/lint"
	},
	"keywords": [
		"prettier",
		"airbnb",
		"eslint",
		"jest"
	],
	"scripts": {
		"postinstall": "node request.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"request": "^2.88.0"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"cross-env": "^6.0.3",
		"eslint": "^6.6.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.7.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jest": "^23.0.4",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"eslint-plugin-react-hooks": "^2.3.0",
		"husky": "^3.1.0",
		"jest": "^24.7.0",
		"jest-in-case": "^1.0.2",
		"lint-staged": "^9.4.3",
		"prettier": "^1.16.4"
	},
	"peerDependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"cross-env": "^6.0.3",
		"eslint": "^6.6.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.7.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jest": "^23.0.4",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"eslint-plugin-react-hooks": "^2.3.0",
		"husky": "^3.1.0",
		"jest": "^24.7.0",
		"jest-in-case": "^1.0.2",
		"lint-staged": "^9.4.3",
		"prettier": "^1.16.4"
	}
}
